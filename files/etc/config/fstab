config 'global'
        option  anon_swap       '0'
        option  anon_mount      '0'
        option  auto_swap       '1'
        option  auto_mount      '1'
        option  delay_root      '5'
        option  check_fs        '0'
        option 'from_fstab'     '1'

config 'mount'
        option  target          '/www/share/usb'
        option  device          '/dev/sda1'
        option 'enabled_fsck'   '0'
