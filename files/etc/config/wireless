
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path '10180000.wmac'
	option htmode 'HT20'

config wifi-iface
	option device 'radio0'
	option network 'wifi'
	option encryption 'psk2'
	option key 'l1ttl35n1pp3r'
	option ssid 'unplug_ACFA'
	option macaddr '00:17:A5:D9:AC:FA'
	option channel '6'
	option mode 'ap'
	option disabled '1'

